@model List<Dictionary<string, object>>

<h1>Appointments</h1>

<table class="table">
    <thead>
        <tr>
            <th>Service</th>
            <th>Time</th>
            <th>Date</th>
            <th>Created At</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var appointment in Model)
        {
            <tr>
                <td>@(appointment.ContainsKey("service") ? appointment["service"] : "N/A")</td>
                <td>@(appointment.ContainsKey("time") ? appointment["time"] : "N/A")</td>
                <td>@(appointment.ContainsKey("date") ? appointment["date"] : "N/A")</td>
                <td>@(appointment.ContainsKey("created_at") ? appointment["created_at"] : "N/A")</td>
            </tr>
        }
    </tbody>
</table>

